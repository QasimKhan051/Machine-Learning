    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "features=(np.random.random([30,2])*20)-10\n",
    "feature1=(features[:,0])\n",
    "feature2=(features[:,1])\n",
    "print(feature1)\n",
    "print(feature2)\n",
    "label=(feature1+feature2)*2\n",
    "print(label)\n",
    "plt.scatter(feature1,label)\n",
    "plt.scatter(feature2,label)\n",
    "\n"
   ]
